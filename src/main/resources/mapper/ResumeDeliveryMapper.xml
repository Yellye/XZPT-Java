<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xz.fzu.dao.IResumeDelivery">
    <resultMap id="myResuleMap" type="xz.fzu.model.ResumeDelivery">
        <result property="resumeDeliveryId" column="resumeDelivery_id"/>
        <result property="recruitmentId" column="recruitment_id"/>
        <result property="userId" column="user_id"/>
        <result property="resumeId" column="resume_id"/>
        <result property="deliveryStatus" column="delivery_status"/>
        <result property="remark" column="remark"/>
    </resultMap>
    <insert id="insertInstance" parameterType="xz.fzu.model.ResumeDelivery">
        INSERT  INTO resume_delivery(recruitment_id,user_id,resume_id,delivery_status,remark)
        VALUES(#{resumeDeliveryId},#{recruitmentId},#{userId},#{resumeId},#{deliveryStatus},#{remark})
    </insert>
</mapper>